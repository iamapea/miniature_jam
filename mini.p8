-- game state and util -------
function update_state()
 if state == "play" then
  --only surviving player wins
  i_existing = {}
  for i = 1,#players do
   if players[i].exists then
    add(i_existing,i)
   end
  end
  if #i_existing == 1 then
   music(0)
   winner = i_existing[1]
   state = "over"
  end
 end
 -- cleanup
 for p in all(players) do
  if (p.lives < 0) p.lives = 0
 end
end

function distance(a,b)
 return sqrt((a.x - b.x)^2 +
             (a.y - b.y)^2)
end


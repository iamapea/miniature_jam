function handle_input()
 for i = 0,#players-1 do
  if state == "title" then
   if btn(‚ùé,i) or
      btn(üÖæÔ∏è,i) then
    state = "pan"
   end
  elseif state == "over" then
   if btn(‚ùé,i) or
      btn(üÖæÔ∏è,i) then
    state = "play"
    restart()
   end
  elseif state == "play" then
   -- i: 0-indexed
   p = players[i+1]
   -- accelerate x
 	 if (btn(‚¨ÖÔ∏è,i) and
 	 	  not btn(‚û°Ô∏è,i)) then
 	 	if (p.aim<0) p.aim=-p.aim
 	  p.vx = -player_speed_x
    if (collision_left(p)) p.vx=0
 	 elseif (btn(‚û°Ô∏è,i) and
 	 	      not btn(‚¨ÖÔ∏è,i)) then
 	 	if (p.aim>0) p.aim=-p.aim
    p.vx = player_speed_x
    if (collision_right(p)) p.vx=0
 	 else
 	  p.vx = 0
 	 end
 	 -- aim
 	 if btn(‚¨ÜÔ∏è,i) then
 	  if p.aim>0+aim_speed and
 	     p.aim<1 then
     p.aim -= aim_speed
    elseif p.aim < -aim_speed then
 	   p.aim += aim_speed
 	  end
 	 elseif btn(‚¨áÔ∏è,i) then
 	  if p.aim>0 and
 	     p.aim<1-aim_speed then
 	   p.aim += aim_speed
    elseif p.aim<0 and p.aim>-1+aim_speed then
     p.aim -= aim_speed
    end
 	 end
 	 -- charge
 	 if btn(‚ùé,i) and
 	    p.cooldown == 0 then
 	  if p.charge <= charge_max then
 	   p.charge += charge_per_frame
 	   p.charges = true
 	  else
 	   --todo: shoot
 	  end
 	 end
 	 if not btn(‚ùé,i) and
 	    p.charges then
  	 --shoot
 	  add(bullets,
 	      new_bullet(p.x,p.y,bullet_speed*p.charge,
 	                 col_bullet,p.aim))
 	  p.cooldown += cooldown_bullet
 	  p.charges = false
 	  p.charge = charge_start
 	  sfx(1)
 	 end
 	 -- jump
 	 if not p.is_airborne and
 	    not p.jumps and
 	    btn(üÖæÔ∏è,i) then
 	  p.jumps = true
 	  p.is_airborne = true
 	  p.vy = - jump_speed
 	  sfx(0)
 	 end
 	end --if state play
	end --for i in 1,#players
end
